{% extends 'base.html.twig' %}

{% block title %}Destinations{% endblock %}

{% block body %}
    <h1>Destinations</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ destination.id }}</td>
            </tr>
            <tr>
                <th>Nom_de_la_destination</th>
                <td>{{ destination.NomDeLaDestination }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ destination.Description }}</td>
            </tr>
            <tr>
                <th>Pays</th>
                <td>{{ destination.Pays }}</td>
            </tr>
            <tr>
                <th>Ville</th>
                <td>{{ destination.Ville }}</td>
            </tr>
            <tr>
                <th>Autres_informations</th>
                <td>{{ destination.AutresInformations }}</td>
            </tr>
            <tr>
                <th>Coordones_geographiques</th>
                <td>{{ destination.CoordonesGeographiques }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_destinations_index') }}">back to list</a>

    <a href="{{ path('app_destinations_edit', {'id': destination.id}) }}">edit</a>

    {{ include('destinations/_delete_form.html.twig') }}
{% endblock %}
